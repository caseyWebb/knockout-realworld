{
  "name": "knockout-realworld",
  "version": "0.0.0",
  "repository": "https://github.com/caseyWebb/knockout-realworld",
  "author": "Casey Webb <notcaseywebb@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest",
    "build": "./scripts/build.sh",
    "develop": "./scripts/develop.sh",
    "deploy": "./scripts/deploy.sh"
  },
  "dependencies": {
    "@profiscience/knockout-contrib-filters": "^0.0.2",
    "@profiscience/knockout-contrib-framework": "^1.0.0-alpha.5",
    "@profiscience/knockout-contrib-model": "^1.0.0-alpha.8",
    "@profiscience/knockout-contrib-observable-fn": "^1.0.0-alpha.10",
    "@profiscience/knockout-contrib-querystring": "^0.0.1-alpha.8",
    "@profiscience/knockout-contrib-router": "^2.0.0-0",
    "knockout": "^3.5.0-beta",
    "knockout-punches": "^0.5.1"
  },
  "devDependencies": {
    "@types/jest": "^22.1.2",
    "@types/knockout": "^3.4.49",
    "@types/node": "^9.4.0",
    "fork-ts-checker-webpack-plugin": "^0.3.0",
    "happypack": "^4.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.3.0",
    "jest-fetch-mock": "^1.4.1",
    "jest-localstorage-mock": "^2.2.0",
    "jest-raw-loader": "^1.0.1",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "ts-jest": "^22.0.4",
    "ts-loader": "^3.4.0",
    "typescript": "^2.7.1",
    "webpack": "^3.10.0",
    "webpack-cli": "^2.0.4",
    "webpack-dev-server": "^2.11.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "mapCoverage": true,
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "moduleNameMapper": {
      "^lib/(.*)$": "<rootDir>/src/lib/$1"
    },
    "setupFiles": ["jest-localstorage-mock"],
    "testRegex": "(__tests__/[^_].*|test)\\.ts$",
    "transform": {
      "^.+\\.[tj]s$": "ts-jest",
      "^.+\\.html$": "jest-raw-loader"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!@profiscience)"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.node.json"
      }
    }
  }
}
